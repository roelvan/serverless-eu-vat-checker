{
  "version": 2,
  "name": "vat-checker",
  "regions": [
    "bru1"
  ],
  "builds": [
    {
      "src": "*.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "methods": [
        "GET",
        "OPTIONS"
      ],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "cache-control": "s-maxage=86400, max-age=0"
      },
      "dest": "/index.js?vat=$1"
    }
  ]
}